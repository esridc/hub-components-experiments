{
  "name": "@esri/radar",
  "version": "0.1.15",
  "description": "ArcGIS Hub components for local exploration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/radar/radar.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "watch": "stencil build --docs --watch",
    "storybook.run": "start-storybook --port 9001",
    "storybook": "npm-run-all --parallel watch storybook.run",
    "publish-npm": "npm publish --access public",
    "publish-storybook": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
