{
  "name": "@esri/radar",
  "version": "0.2.2",
  "description": "ArcGIS Hub components for local exploration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/radar/radar.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "watch": "stencil build --docs --watch",
    "storybook.run": "start-storybook --port 9001 -s ./public,./dist",
    "storybook-ssl.run": "start-storybook --port 9001 -s ./public --https --ssl-cert ~/Downloads/certs/localui.arcgis.com.crt --ssl-ca ~/Downloads/certs/myCA.pem --ssl-key ~/Downloads/certs/localui.arcgis.com.key",
    "storybook": "npm-run-all  --parallel watch storybook.run",
    "publish-npm": "npm publish --access public",
    "publish-storybook": "storybook-to-ghpages -s build-storybook",
    "build-storybook": "build-storybook -s ./public,./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@stencil/core": "^1.13.0",
    "@stencil/sass": "^1.3.1",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/jest": "^25.2.2",
    "@types/puppeteer": "2.0.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__html": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "case": "^1.6.3",
    "glob": "^7.1.6",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "2.1.1",
    "storybook": "^5.3.18",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajturner/radar.git"
  },
  "contributors": [
    {
      "name": "Andrew Turner",
      "email": "aturner@esri.com"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.1",
    "@esri/arcgis-rest-auth": "^2.13.1",
    "@esri/arcgis-rest-common-types": "^1.19.2",
    "@esri/arcgis-rest-feature-layer": "^2.13.1",
    "@esri/arcgis-rest-geocoding": "^2.13.1",
    "@esri/arcgis-rest-portal": "^2.13.1",
    "@esri/arcgis-rest-request": "^2.13.1",
    "@esri/arcgis-rest-service-admin": "^2.13.2",
    "@esri/arcgis-rest-types": "^2.13.1",
    "@esri/calcite-components": "^1.0.0-beta.30",
    "@esri/calcite-ui-icons": "^3.3.0",
    "@esri/hub-annotations": "^4.2.2",
    "@esri/hub-common": "^4.0.0",
    "@esri/hub-events": "^4.0.0",
    "@esri/hub-initiatives": "^4.1.0",
    "@esri/hub-search": "^4.1.0",
    "@esri/hub-sites": "^4.1.0",
    "@esri/telemetry": "^1.5.1",
    "@storybook/addon-a11y": "^5.3.19",
    "@types/arcgis-js-api": "^4.15.0",
    "@types/node": "^13.13.6",
    "date-fns": "^2.15.0",
    "esri-loader": "^2.14.0",
    "eventemitter3": "^4.0.4",
    "fuse.js": "^5.2.3",
    "i18next": "^19.6.2",
    "lodash-es": "^4.17.15",
    "node-polyglot": "^2.4.0"
  }
}
